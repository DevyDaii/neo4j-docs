<ivysettings> 
  <settings defaultResolver="main"/> 
  <resolvers> 
    <chain name="main"> 
      <filesystem name="local"> 
        <artifact pattern="${ivy.settings.dir}/repository/[artifact]-[revision].[ext]" /> 
      </filesystem> 
      <ibiblio name="maven_central" root="http://repo1.maven.org/maven2/" m2compatible="true"/> 
    </chain> 
  </resolvers> 
</ivysettings>
